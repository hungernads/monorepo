{
	"name": "hungernads",
	"version": "0.1.0",
	"private": true,
	"description": "AI gladiator colosseum on Monad - May the nads be ever in your favor",
	"scripts": {
		"start:dev": "wrangler dev",
		"deploy": "wrangler deploy",
		"typecheck": "tsc --noEmit",
		"test": "tsx tests/battle-flow.test.ts && tsx tests/betting.test.ts && tsx tests/api.test.ts",
		"test:battle": "tsx tests/battle-flow.test.ts",
		"test:betting": "tsx tests/betting.test.ts",
		"test:api": "tsx tests/api.test.ts",
		"test:llm": "npx tsx src/llm/test-providers.ts",
		"db:migrate": "wrangler d1 migrations apply hungernads-db --local",
		"battle": "tsx scripts/run-battle.ts",
		"replay": "tsx scripts/generate-replay.ts",
		"replay:html": "tsx scripts/generate-replay.ts --html-only",
		"launch-token": "tsx scripts/launch-token.ts"
	},
	"dependencies": {
		"@ai-sdk/openai": "^1.1.0",
		"@cloudflare/unenv-preset": "^2.12.0",
		"@nadfun/sdk": "^0.4.3",
		"ai": "^4.1.0",
		"hono": "^4.6.0",
		"openai": "^4.77.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"viem": "^2.21.0",
		"zod": "^3.24.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20241230.0",
		"dotenv": "^17.2.4",
		"tsx": "^4.19.0",
		"typescript": "^5.7.0",
		"wrangler": "^4.63.0"
	}
}
